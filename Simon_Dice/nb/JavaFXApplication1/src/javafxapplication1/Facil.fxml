<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane id="PanelFacil" prefHeight="450.0" prefWidth="450.0"
        xmlns="http://javafx.com/javafx/11.0.1"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="javafxapplication1.FXMLDocumentController">
    <children>
        <Button id="boton_0" fx:id="boton0" layoutX="50.0" layoutY="50.0"
            onAction="#handleButtonAction" onMouseClicked="#picaO"
            prefHeight="150.0" prefWidth="150.0">
         <graphic>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE"
                height="150.0" stroke="BLACK" strokeType="INSIDE"
                width="150.0" />
         </graphic></Button>
      <Button id="boton_1" fx:id="boton1" layoutX="250.0" layoutY="50.0"
            onAction="#handleButtonAction" onMouseClicked="#picaI"
            prefHeight="150.0" prefWidth="150.0">
         <graphic>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#f82727"
                height="150.0" stroke="BLACK" strokeType="INSIDE"
                width="150.0" />
         </graphic>
      </Button>
      <Button id="boton_2" fx:id="boton2" layoutX="50.0" layoutY="250.0"
            onAction="#handleButtonAction" onMouseClicked="#picaO"
            prefHeight="150.0" prefWidth="150.0">
         <graphic>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#3ffcd6"
                height="150.0" stroke="BLACK" strokeType="INSIDE"
                width="150.0" />
         </graphic>
      </Button>
      <Button id="boton_3" fx:id="boton3" layoutX="250.0" layoutY="250.0"
            onAction="#handleButtonAction" onMouseClicked="#picaO"
            prefHeight="150.0" prefWidth="150.0">
         <graphic>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#cd04c9"
                height="150.0" stroke="BLACK" strokeType="INSIDE"
                width="150.0" />
         </graphic>
      </Button>
    </children>
</AnchorPane>
